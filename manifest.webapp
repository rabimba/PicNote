{
  "version": "1.0",
  "name": "PicNote",
  "description": "PicNote is a Mobile Web App that provides the user to make note with photos",
  "type": "privileged",
  "launch_path": "/index.html",
  "developer": {
    "name": "Rabimba Karanjai",
    "url": "https://rabimba.com"
  },
  "permissions": {
    "geolocation": {},
    "desktop-notification":{},
    "systemXHR": {},
    "device-storage:pictures": { "access": "readonly" },
    "contacts":{ "access": "readcreate" }
  },
  "orientation": "portrait-primary",
  "locales": {
    "en-US": {
      "name": "PicNote",
      "description": "PicNote is a Mobile Web App that provides the user to make note with photos"
    }
  },
  "default_locale": "en-US",
  "icons": {
    "128": "img/logo.png"
  }
}
